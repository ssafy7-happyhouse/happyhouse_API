<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.happyhouse.model.mapper.ApartmentDealMapper">

	<select id="dealList" parameterType="integer" resultType="ApartmentDeal">
		select dealAmount, dealYear, dealMonth, area, floor
		from housedeal
		where aptCode=#{aptCode} 
		order by dealYear desc
	</select>
	

</mapper>
